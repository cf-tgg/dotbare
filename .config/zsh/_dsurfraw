#compdef dsurfraw

BROWSERS=(
  brave
  firefox
  chromium
  librewolf
  qutebrowser
  tor-browser-launcher
  firefox-developers-edition
)

ELVIS=(
  google
  duckduckgo
  archwiki
  emacswiki
  wikipedia
  youtube
  discogs
  imdb
)

_dsurfraw() {
    set -e
    sr --elvi
    reply=("$@")
}

compdef _dsurfraw dsurfraw
